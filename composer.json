{
  "name": "mitchellmacpherson/api-skeleton",
  "description": "Base for new API projects",
  "keywords": [],
  "license": "Proprietary",
  "type": "project",
  "require": {
    "ext-json": "*",
    "php": ">=7.2",
    "vlucas/phpdotenv": "~2.2",
    "php-di/php-di": "^6.0",
    "symfony/console": "^4.1",
    "ext-gd": "*"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
